    style InventoryAgent fill:#e1ffe1,stroke:#333,stroke-width:2px
    style QuoteAgent fill:#fff5e1,stroke:#333,stroke-width:2px
    style SalesAgent fill:#f5e1ff,stroke:#333,stroke-width:2px
